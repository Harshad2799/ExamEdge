package com.abm.examedge.controller;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.RequestParam;
import org.springframework.web.bind.annotation.RestController;

import com.abm.examedge.entity.StudentDetails;
import com.abm.examedge.service.studentService;

@RestController
public class studentController {
	
	@Autowired
	private studentService stuservice;
	
	public List<StudentDetails> findStudent(@RequestParam int id){
		List<StudentDetails> list = stuservice.editDetails(id);
	}
}
