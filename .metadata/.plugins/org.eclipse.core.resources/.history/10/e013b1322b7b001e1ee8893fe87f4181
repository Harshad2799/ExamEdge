package com.abm.examedge.service;

import java.util.List;
import java.util.Optional;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.abm.examedge.interfaces.studentDetailsInterface;
import com.abm.examedge.entity.StudentDetails;

@Service
public class studentService {

	@Autowired
	private studentDetailsInterface sturepo;
	
	public Optional<StudentDetails> editDetails(int id) {
		return sturepo.findById(id);	
	}
	
	public void updaterecord(int id, StudentDetails stu) {
		sturepo.save(stu);
	}
}
